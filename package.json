{
  "name": "@kazaar/express-error-handler",
  "version": "2.2.9",
  "description": "Express error handling and logging utilities",
  "main": "index.js",
  "repository": "arthurfauq/express-error-handler",
  "homepage": "https://github.com/arthurfauq/express-error-handler",
  "author": {
    "name": "Arthur Fauquenot",
    "email": "arthurfauquenot@yahoo.com",
    "url": "https://github.com/arthurfauq"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "nodejs",
    "rest",
    "rest-api",
    "restful",
    "express",
    "errors",
    "http-errors"
  ],
  "scripts": {
    "lint": "eslint index.js && prettier --check \"**/*.{json,md,html,yaml}\" --ignore-path .gitignore",
    "lint:fix": "eslint index.js --fix && prettier --write \"**/*.{json,md,html,yaml}\" --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,html,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "celebrate": "10.0.0",
    "chalk": "^2.4.2"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0"
  },
  "license": "MIT"
}
