{
  "name": "@kazaar/express-error-handler",
  "version": "3.0.0",
  "description": "Express error handling and logging utilities",
  "main": "index.js",
  "repository": "arthurfauq/express-error-handler",
  "homepage": "https://github.com/arthurfauq/express-error-handler",
  "author": {
    "name": "Arthur Fauquenot",
    "email": "arthurfauquenot@gmail.com",
    "url": "https://github.com/arthurfauq"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "types": "index.d.ts",
  "keywords": [
    "nodejs",
    "express",
    "middleware",
    "http-errors",
    "errors",
    "rest"
  ],
  "scripts": {
    "lint": "eslint index.js && prettier --check \"**/*.{json,md,html,yaml}\" --ignore-path .gitignore",
    "lint:fix": "eslint index.js --fix && prettier --write \"**/*.{json,md,html,yaml}\" --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,html,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "celebrate": "10.0.0"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2"
  },
  "peerDependencies": {
    "express": ">= 4.x"
  },
  "license": "MIT"
}
